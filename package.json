{
  "name": "saga",
  "version": "0.1.4",
  "description": "Saga ",
  "main": "index.js",
  "scripts": {
    "start": "rm -rf _site & eleventy --serve & npm run tw:watch",
    "admin": "npx netlify-cms-proxy-server & npm run start && open:admin",
    "clean": "rm -rf _site & rm & rm src/assets/css/style.css & rm  src/assets/css/style.min.css",
    "tw:watch": "npx tailwindcss -i tailwind.css -o src/assets/css/style.css --watch",
    "tw:build": "npx tailwindcss -i tailwind.css -o src/assets/css/style.min.css --minify",
    "netlify": "netlify dev",
    "debug": "DEBUG=* eleventy",
    "build": "npm run tw:build && npm run debug "
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@11ty/eleventy": "^0.12.1"
  },
  "devDependencies": {
    "@11ty/eleventy-img": "^2.0.1",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.1.0",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/typography": "^0.5.4",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.12",
    "tailwindcss": "^3.1.6"
  }
}
