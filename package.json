{
  "name": "saga",
  "version": "0.1.4",
  "description": "Saga ",
  "main": "index.js",
  "scripts": {
    "start": "npx @11ty/eleventy --serve & npm run tw:watch",
    "admin": "npx netlify-cms-proxy-server & npm run start",
    "tw:watch": "npx tailwindcss -i tailwind.css -o src/assets/css/style.css --watch",
    "tw:build": "npx tailwindcss -i tailwind.css -o src/assets/css/style.min.css --minify",
    "clean build": "rm -rf _site & rm src/assets/css/style.css",
    "netlify": "netlify dev",
    "debug": "ELEVENTY_ENV=prod DEBUG=* eleventy",
    "build": "npm run tw:build && npm run debug "
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.1",
    "@11ty/eleventy-img": "^2.0.1",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.1.0",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/typography": "^0.5.4",
    "luxon": "^3.0.1",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.12",
    "tailwindcss": "^3.1.6"
  }
}
