{# ALL TAGS
------------------------------------------------
List of all tags
#}
{%- if meta.env != 'prod' %}<!-- nav/tags.njk  -->{%- endif  -%}

{% for tag in collections.tagList   %}
  <nav aria-label="All Tags">
    {% for item in collections.all |  getPage("/tag/" + tag + "/" ) %}
      <a href="/tag/{{ tag }}" class="block"> {{ item.data.title}}</a>
    {% endfor %}
  </nav>
{% endfor %}
